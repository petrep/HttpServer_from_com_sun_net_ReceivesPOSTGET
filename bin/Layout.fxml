<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
    
    <Label alignment="TOP_LEFT" text="HTTP Server" />
    <Button alignment="CENTER" nodeOrientation="LEFT_TO_RIGHT" onAction="#startserver" text="StartServer" />
   <Button mnemonicParsing="false" onAction="#stopserver" text="StopServer" />
   <TextArea fx:id="consoleoutput" prefHeight="314.0" prefWidth="600.0" />
    
</VBox>
